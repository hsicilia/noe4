{% extends 'base.html.twig' %}

{% set menu='informe' %}

{% block title %}{{ 'title.informe.ejemplares' | trans }}{% endblock %}

{% block titular %}{{ 'title.informe.ejemplares' | trans }}{% endblock %}

{% block contenido %}
    <fieldset>
        <legend>{{ 'informe.mensaje.personalizado' | trans }}</legend>
        {{ form_start(formulario) }}
            {{ form_errors(formulario) }}
            {{ form_row(formulario.fechaInicio) }}
            {{ form_row(formulario.fechaFin) }}
            {{ form_row(formulario.invasores) }}
            {{ form_row(formulario.cites) }}
            {{ form_row(formulario.buscar) }}
        {{ form_end(formulario) }}
    </fieldset>
{% endblock %}
